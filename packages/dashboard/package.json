{
  "name": "@stacksjs/httx-dashboard",
  "type": "module",
  "version": "0.1.0",
  "description": "A real-time monitoring dashboard for httx HTTP client, built with stx.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/httx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/httx.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/httx/issues"
  },
  "keywords": [
    "httx",
    "dashboard",
    "monitoring",
    "stx",
    "http-client",
    "metrics"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE.md",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun --bun build.ts",
    "dev": "bunx bun-plugin-stx serve src/pages/ --port 4401",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "bun tsc --noEmit"
  },
  "dependencies": {
    "@cwcss/crosswind": "^0.1.6",
    "@stacksjs/httx": "^0.1.7"
  },
  "devDependencies": {
    "@stacksjs/stx": "^0.2.5",
    "bun-plugin-stx": "^0.2.5"
  }
}
